format = """
[░▒▓](#8F80B5)\
 $os\
[](bg:#6A51D7 fg:#8F80B5)\
$directory\
[](fg:#6A51D7 bg:#39334C)\
$git_branch\
$git_status\
[](fg:#39334C bg:#201D2B)\
$nodejs\
$rust\
$golang\
$php\
[](fg:#201D2B bg:#1d2230)\
$time\
[ ](fg:#1d2230)\
\n≻ """

[os]
style = "bg:#8F80B5 fg:#090c0c"
disabled = false

[directory]
style = "fg:#e3e5e5 bg:#6A51D7"
format = "[ $path ]($style)"
truncation_length = 3
truncation_symbol = "…/"

[directory.substitutions]
"Documents" = "󰈙 "
"Downloads" = " "
"Music" = " "
"Pictures" = " "

[git_branch]
symbol = ""
style = "bg:#39334C"
format = '[[ $symbol $branch ](fg:#8B7CCD bg:#39334C)]($style)'

[git_status]
style = "bg:#39334C"
format = '[[($all_status$ahead_behind )](fg:#8B7CCD bg:#39334C)]($style)'

[nodejs]
symbol = ""
style = "bg:#201D2B"
format = '[[ $symbol ($version) ](fg:#6A51D7 bg:#201D2B)]($style)'

[rust]
symbol = ""
style = "bg:#201D2B"
format = '[[ $symbol ($version) ](fg:#6A51D7 bg:#201D2B)]($style)'

[golang]
symbol = ""
style = "bg:#201D2B"
format = '[[ $symbol ($version) ](fg:#6A51D7 bg:#201D2B)]($style)'

[php]
symbol = ""
style = "bg:#201D2B"
format = '[[ $symbol ($version) ](fg:#6A51D7 bg:#201D2B)]($style)'

[time]
disabled = false
time_format = "%R" # Hour:Minute Format
style = "bg:#1d2230"
format = '[[  $time ](fg:#a0a9cb bg:#1d2230)]($style)'
